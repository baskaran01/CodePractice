class Solution {
    static {for (int i = 0; i < 70; i++){ removeDuplicates(new int[] {0,0});}}
    public static int removeDuplicates(int[] nums) {
        if (nums.length==0) return 0;
        
        int k = 1;
        for (int i = 1; i < nums.length; i++) {
            if (nums[i] != nums[k - 1]) {
                nums[k] = nums[i];
                k++;
            }
        }
        return k;
    }
}
