int maxArea(vector<int>& height) {
        int Max=0,left=0,right=height.size()-1;
        while(left<right)
        {
            int h=min(height[left],height[right]);
            int w=right-left;
            int a=h*w;
            Max=max(Max,a);
            if(height[left]<height[right])
            {
                left++;
            }
            else
            {
                right--;
            }
        }
        return Max;
    }
