class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        
    int j=0,i=0,k=0;
    for(i=m;i<m+n;i++)
    {     
        nums1[i]=nums2[j++];
    }
    for(i=0;i<m+n;i++)
    {
      for(j=i;j<m+n;j++)
      {
        if(nums1[i]>nums1[j])
        {
            int temp=nums1[i];
            nums1[i]=nums1[j];
            nums1[j]=temp;
        }
      }  
    }
    i=0;    
    while(i<m+n)
    {
        cout<<nums1[i++];

    }
    }
};
