class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
    int n=nums.size();
        if(n==0)
        {
            return 0;
        }
        int i=0;
        int k=n-1;
        while(i<=k)
        {
            if(nums[i]==val)
            {
                nums[i]=nums[k];
                k--;
            }
            else i++;
        }

        return k+1;
    }
};
